Get-WinEvent -FilterHashtable @{logname="Microsoft-Windows-Sysmon/Operational";id=3;} | Where {$_.message -notlike "*224.0.0.252*" -and $_.message -notlike "*Image:*chrome*" -and $_.message -notlike "*EPSecurityService*" -and $_.message -notlike "*SourcePortName: ssdp*" -and $_.message -notlike "*llmnr*" -and $_.message -notlike "*DestinationHostname:*alaskacleanseas.org*" -and $_.message -notlike "*SourceIP:*ff02:0:0:0:0:0:0:fb*" -and $_.message -notlike "*DestinationIP:192.168*" -and $_.message -notlike "*DestinationIP:10.32.32*" -and $_.message -notlike "*DestinationIP:52.109.12.22*" -and $_.message -notlike "*DestinationIP:40.97.*" -and $_.message -notlike "*Image:*bdredline.exe*" -and $_.message -notlike "*OUTLOOK.EXE*" -and $_.message -notlike "*netbios-dgm*" -and $_.message -notlike "*ws-discovery*" -and $_.message -notlike "*netbios-ns*" -and $_.message -notlike "*52.109*" -and $_.message -notlike "*40.83.*" -and $_.message -notlike "*224.0.0.*" -and $_.message -notlike "*Bitdefender*" -and $_.message -notlike "*40.114.238.193*" -and $_.message -notlike "*iexplore.exe*" -and $_.message -notlike "*10.32.32.*" -and $_.message -notlike "*0:0:0:0:0:0:0:1*" -and $_.message -notlike "*PortName: wsd*" -and $_.message -notlike "*239.255.255.250*" -and $_.message -notlike "*googledrivesync.exe*" -and $_.message -notlike "*TeamViewer_Service.exe*" -and $_.message -notlike "*13.89.*" -and $_.message -notlike "*52.165*" -and $_.message -notlike "*52.114*" -and $_.message -notlike "*13.107.*"} | Select-Object -Property message | Format-List > C:\temp\thingstocheckout.txt
